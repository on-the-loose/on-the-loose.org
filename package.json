{
  "name": "otl.pomona.edu",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/core": "^10.0.10",
    "@firebase/app-types": "^0.3.9",
    "@firebase/util": "^0.2.13",
    "@sentry/browser": "^5.1.1",
    "@types/react": "^16.8.14",
    "@types/react-router": "^4.4.5",
    "@types/react-router-dom": "^4.3.2",
    "antd": "^3.16.5",
    "firebase": "^5.10.0",
    "moment": "^2.24.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-firebase-hooks": "^1.2.0",
    "react-ga": "^2.5.7",
    "react-markdown": "^4.0.8",
    "react-photo-gallery": "^7.0.2",
    "react-quill": "^1.3.3",
    "react-router": "^5.0.0",
    "react-router-dom": "^5.0.0",
    "react-youtube": "^7.9.0",
    "safe-html": "^1.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/plugin-transform-react-jsx": "^7.3.0",
    "@babel/preset-typescript": "^7.3.3",
    "babel-plugin-emotion": "^10.0.9",
    "babel-plugin-import": "^1.11.0",
    "babel-plugin-jsx-pragmatic": "^1.0.2",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "parcel-bundler": "^1.12.3",
    "parcel-plugin-babel-typescript": "^1.0.1"
  },
  "alias": {
    "src": "./src"
  },
  "babel": {
    "presets": [
      "@babel/preset-typescript"
    ],
    "plugins": [
      "transform-inline-environment-variables",
      [
        "babel-plugin-jsx-pragmatic",
        {
          "export": "jsx",
          "module": "@emotion/core",
          "import": "___EmotionJSX"
        }
      ],
      [
        "@babel/plugin-transform-react-jsx",
        {
          "pragma": "___EmotionJSX",
          "pragmaFrag": "React.Fragment"
        }
      ],
      "emotion",
      [
        "import",
        {
          "libraryName": "antd",
          "style": "css"
        }
      ]
    ]
  },
  "prettier": {
    "printWidth": 100,
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true
  },
  "scripts": {
    "start": "parcel public/index.html",
    "build": "rm -rf dist; parcel build public/index.html --no-source-maps"
  }
}
